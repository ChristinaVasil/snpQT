process {
  // default container for all processes
  container = "snpqt"
  // custom containers 
  // build conversion
  withName: dictionary {
    container = "openjdk:8"
  }
  withName: liftover {
    container = "openjdk:8"
  }
  // popStrat
  withName: run_snpflip {
    container = "snpflip"
  }
  withName: eigensoft {
    container = "quay.io/biocontainers/eigensoft:7.2.1--h1d3628b_2"
  }
  // imputation
  withName: phasing {
    container = "quay.io/biocontainers/shapeit4:4.2.0--h0c37f25_0"
  }
  // these require the user to build the dockerfile
  withName: convert_imp5 {
    container = "impute5"
  }
  withName: impute5 {
    container = "impute5"
  }
}